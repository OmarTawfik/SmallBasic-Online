<root kind="ExpressionSyntax">
    <UnaryOperatorExpressionSyntax>
        <operatorToken type="Token" />
        <expression type="BaseExpressionSyntax" />
    </UnaryOperatorExpressionSyntax>

    <BinaryOperatorExpressionSyntax>
        <leftExpression type="BaseExpressionSyntax" />
        <operatorToken type="Token" />
        <rightExpression type="BaseExpressionSyntax" />
    </BinaryOperatorExpressionSyntax>

    <ObjectAccessExpressionSyntax>
        <baseExpression type="BaseExpressionSyntax" />
        <dotToken type="Token" />
        <identifierToken type="Token" />
    </ObjectAccessExpressionSyntax>

    <ArrayAccessExpressionSyntax>
        <baseExpression type="BaseExpressionSyntax" />
        <leftBracketToken type="Token" />
        <indexExpression type="BaseExpressionSyntax" />
        <rightBracketToken type="Token" />
    </ArrayAccessExpressionSyntax>

    <CallExpressionSyntax>
        <baseExpression type="BaseExpressionSyntax" />
        <leftParenToken type="Token" />
        <argumentsList type="BaseExpressionSyntax" array="true" />
        <commasList type="Token" array="true" />
        <rightParenToken type="Token" />
    </CallExpressionSyntax>

    <IdentifierExpressionSyntax>
        <name type="string" />
        <token type="Token" />
    </IdentifierExpressionSyntax>

    <NumberLiteralExpressionSyntax>
        <value type="number" />
        <token type="Token" />
    </NumberLiteralExpressionSyntax>

    <StringLiteralExpressionSyntax>
        <value type="string" />
        <token type="Token" />
    </StringLiteralExpressionSyntax>
    
    <ParenthesisExpressionSyntax>
        <leftParenToken type="Token" />
        <expression type="BaseExpressionSyntax" />
        <rightParenToken type="Token" />
    </ParenthesisExpressionSyntax>

    <MissingExpressionSyntax>
        <range type="TextRange" />
    </MissingExpressionSyntax>
</root>